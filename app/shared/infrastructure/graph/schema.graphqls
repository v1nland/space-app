# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Astronaut { 
  id: ID!
  name: String!
  isPilot: Boolean!
}

type Mission {
  id: ID!
  name: String!
  crew: [Astronaut]!
}

type Query {
  getAstronautById(id: ID!): Astronaut
}

type Mutation {
  createAstronaut(input: NewAstronaut!): ID
}

input NewAstronaut {
  name: String!
  isPilot: Boolean!
}
